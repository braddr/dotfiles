Host github.com
        User git
        Hostname github.com
        PreferredAuthentications publickey
        IdentityFile ~/.ssh/puremagic-github.rsa

Host *.puremagic.com
        User braddr
        IdentityFile ~/.ssh/puremagic-id.rsa

Host *.compute-1.amazonaws.com
        User ec2-user
        PreferredAuthentications publickey
        IdentityFile ~/.ssh/cygwin-keypair-one.pem
